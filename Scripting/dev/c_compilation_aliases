############################# Compilation Templates #######################
#gcc compuliation aliases:c89, c99, debug mode, release mode
alias gd='gcc -ansi -pedantic-errors -Wall -Wextra -g'
alias gc='gcc -ansi -pedantic-errors -Wall -Wextra -DNDBUG -03'
alias gc='gcc -std=c99 -pedantic-errors -Wall -Wextra -g'
alias gc='gcc -std=c99 -pedantic-errors -Wall -Wextra -DNDBUG -03'
#
############################# Valgrind Templates #######################
alias vlg='valgrind --leak-check=yes --track-origins=yes' 
alias svlg='valgrind -s --leak-check=full --track-origins=yes' 

